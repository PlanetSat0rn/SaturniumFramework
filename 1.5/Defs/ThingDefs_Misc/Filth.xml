<Defs>
  <ThingDef ParentName="BaseFilth" Name="FilthBlood">
    <defName>Filth_GalacticSplatter</defName>
    <label>galactic splatter</label>
    <statBases>
      <Beauty>-25</Beauty>
      <Cleanliness>-10</Cleanliness>
    </statBases>
    <graphicData>
      <texPath>Things/Filth/Spatter</texPath>
      <color>(68, 0, 150, 180)</color>
    </graphicData>
    <filth>
      <ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
      <disappearsInDays>35~40</disappearsInDays>
      <rainWashes>true</rainWashes>
      <cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
      <canFilthAttach>true</canFilthAttach>
      <cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
    </filth>
  </ThingDef>

  <ThingDef ParentName="FilthBlood">
    <defName>Filth_GalacticSmear</defName>
    <label>galactic smear</label>
    <graphicData>
      <texPath>Things/Filth/CrawlSmear</texPath>
      <color>(68, 0, 150, 165)</color>
      <graphicClass>Graphic_ClusterTight</graphicClass>
      <drawSize>0.9</drawSize>
    </graphicData>
  </ThingDef>
</Defs>